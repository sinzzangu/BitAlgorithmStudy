   "source": [
    "def solution(w,h):\n",
    "    total=0\n",
    "    a=max(w,h)\n",
    "    b=min(w,h)\n",
    "    for i in range(1,(b//2)+1):\n",
    "        if i*a/b==int(i*a/b):\n",
    "            total=((i*a/b)+i-1)*b/i\n",
    "            break\n",
    "\n",
    "    if total==0:\n",
    "        total=w+h-1\n",
    "    \n",
    "    return w*h-total"
   ]
