   "source": [
    "def solution(n):\n",
    "    return hanoi(n,1,2,3)\n",
    "        \n",
    "        \n",
    "def hanoi(n,sta,bet,end):\n",
    "    if n==1:\n",
    "        return [[sta,end]]\n",
    "    else:\n",
    "        return hanoi(n-1,sta,end,bet) + [[sta,end]] + hanoi(n-1,bet,sta,end)"
   ]
